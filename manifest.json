  {
    "name": "WebWork Utilities Chrome Extension",
    "version": "1.0",
    "description": "Chrome extension for WebWork Utilities",
	"permissions": ["activeTab", "declarativeContent", "storage", "tabs"],
	"background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "content_scripts": [{
      "css": ["jquery-ui-1.12.1.custom/jquery-ui.theme.css", "styles.css"],
      "js": ["jquery-3.3.1.js", "jquery-ui-1.12.1.custom/jquery-ui.js","content.js"],
      "matches": ["https://webwork.elearning.ubc.ca/*"]
    }],
	"page_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
      }
    },
	"icons": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    },
    "manifest_version": 2
  }